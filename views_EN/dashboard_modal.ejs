						<% } %>
					<% } %>
				</optgroup>
				<optgroup label="Dropbox">
					<% if (user.dropbox.length > 0) { %>          
						<% for(var i = 0; i < user.dropbox.length; i++) { %>
							<option class="dropbox" value="<%= user.dropbox[i]._id %>"><%= user.dropbox[i].email %></option>
						<% } %>
					<% } %>
